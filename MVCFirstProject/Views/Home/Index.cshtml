<h4>Employee List using ViewData</h4>
@if (ViewData["emplist"] != null)
{
    <table class="table table-bordered">
        <tr>
            <th>Name </th>
            <th>City</th>
            <th>Salary</th>
        </tr>
        @foreach (Employee e in (IEnumerable<Employee>)ViewData["emplist"])
        {
            <tr>
                <td>@e.Name</td>
                <td>@e.City</td>
                <td>@e.Salary</td>
            </tr>
        }
    </table>
}